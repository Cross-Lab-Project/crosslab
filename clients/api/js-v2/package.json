{
  "name": "@cross-lab-project/api-client",
  "version": "0.0.1",
  "description": "API-Client for all Crosslab APIs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/Cross-Lab-Project/crosslab.git"
  },
  "scripts": {
    "build-generate": "npx openapi-codegen -i ../../../services/openapi/dist/openapi.json -p @cross-lab-project/codegen-typescript-addon:preset:client -o ./src/generated",
    "build-compile": "tsc",
    "build": "npm run build-generate && npm run build-compile",
    "prepare": "npm run build",
    "test": "node dist/test/test.js",
    "format": "npx prettier src --write",
    "predocs": "npm run build",
    "docs": "typedoc --options ./typedoc.json"
  },
  "files": [
    "dist"
  ],
  "exports": "./dist/index.js",
  "author": "Pierre Helbing",
  "license": "ISC",
  "devDependencies": {
    "@cross-lab-project/codegen-typescript-addon": "file:../../../helper/crosslab-typescript-addon",
    "@cross-lab-project/openapi-codegen": "file:../../../helper/openapi-codegeneration-v2/cross-lab-project-openapi-codegen-0.2.0.tgz",
    "@types/node": "^18.0.3",
    "typedoc": "^0.23.20",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@cross-lab-project/documentation-theme": "file:../../../helper/documentation-theme/cross-lab-project-documentation-theme-0.0.1.tgz",
    "ajv-formats": "^2.1.1",
    "cross-fetch": "^3.1.5"
  }
}
