{
  "$schema": "https://typedoc.org/schema.json",
  "entryPointStrategy": "expand",
  "entryPoints": ["./src"],
  "out": "docs",
  "excludeInternal": true,
  "exclude": [
    "./src/generated/basicValidation.ts",
    "./src/generated/basicValidation/*",
    "./src/generated/requestValidation.ts"
  ],
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeExternals": true,
  "categorizeByGroup": false,
  "categoryOrder": ["Constructors", "Accessors", "*"],
  "disableSources": true,
  "plugin": ["@cross-lab-project/tsdoc-theme"],
  "theme": "crosslab"
}
