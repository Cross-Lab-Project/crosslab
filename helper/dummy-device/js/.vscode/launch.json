    "version": "0.2.0",
    "configurations": [
        {
            "name": "Browser",
            "request": "attach",
            "type": "chrome",
            "port": 9301,
            "webRoot": "${workspaceFolder}/http-dist",
            "urlFilter": "*index.html",
            "restart": false,
            "timeout": 1000,
        },
        {
            "name": "Node",
            "type": "node",
            "request": "attach",
            "port": 9301,
            "restart": false,
            "timeout": 1000,
        }
    ]
}