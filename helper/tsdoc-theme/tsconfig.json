{
    "compilerOptions": {
      "target": "es2020",
      "module": "commonjs",
      "moduleResolution": "node",
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "outDir": "lib",
      "esModuleInterop": true,
  
      "jsx": "react",
      "jsxFactory": "JSX.createElement",
      "jsxFragmentFactory": "JSX.Fragment",
    },
    "include": ["src"]
}